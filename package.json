{
  "name": "@buildit/storybook-addon-versions",
  "version": "0.0.1",
  "description": "Navigate different versions of storybook style guides.",
  "main": "dist/index.js",
  "bin": {
    "blabbr-config": "./scripts/configure.js"
  },
  "scripts": {
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001 -s ./.storybook",
    "lint": "eslint src --ext .js",
    "transpile": "babel ./src --out-dir dist --copy-files",
    "test": "jest",
    "build": "npm run lint && npm run test && npm run transpile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildit/storybook-addon-versions.git"
  },
  "bugs": {
    "url": "https://github.com/buildit/storybook-addon-versions/issues"
  },
  "homepage": "https://github.com/buildit/storybook-addon-versions#readme",
  "keywords": [
    "atomic-design",
    "blabbr",
    "react",
    "storybook",
    "storybook-addon",
    "storybook-addon-blabbr",
    "collaboration",
    "review"
  ],
  "author": "Buildit",
  "license": "MIT",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "blabbr-config": "<rootDir>/.storybook/blabbr-config.js"
    }
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.2",
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.17.0",
    "webpack": "^2.6.1"
  },
  "peerDependencies": {
    "@kadira/storybook": "^2.35.2",
    "@kadira/storybook-addons": "^1.6.1",
    "css-loader": "^0.28.1",
    "prop-types": "^15",
    "react": "^15",
    "react-dom": "^15",
    "style-loader": "^0.17.0"
  }
}
